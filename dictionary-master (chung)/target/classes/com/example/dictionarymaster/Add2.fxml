        <ImageView fitHeight="55.0" fitWidth="850.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/header.png" />
            </image></ImageView>
        <Label layoutX="284.0" layoutY="114.0" text="Create a new word">
            <font>
                <Font size="39.0" />
            </font>
        </Label>
        <Label layoutX="148.0" layoutY="205.0" text="New word">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label layoutX="179.0" layoutY="286.0" text="Mean">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="addWord" layoutX="345.0" layoutY="212.0" prefHeight="31.0" prefWidth="314.0" promptText="Your answer" />
        <TextField fx:id="addMean" layoutX="345.0" layoutY="293.0" prefHeight="31.0" prefWidth="314.0" promptText="Your answer" />
