<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionarymaster.editWordController">
    <children>
        <MenuBar prefHeight="32.0" prefWidth="850.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <ImageView fitHeight="55.0" fitWidth="850.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/header.png" />
            </image></ImageView>
        <Label layoutX="301.0" layoutY="114.0" text="Editing a word">
            <font>
                <Font size="39.0" />
            </font>
        </Label>
        <Label layoutX="165.0" layoutY="205.0" text="Word">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label layoutX="110.0" layoutY="286.0" text="New meaning">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="editWord" layoutX="332.0" layoutY="212.0" prefHeight="31.0" prefWidth="314.0" />
        <TextField fx:id="editNewMean" layoutX="332.0" layoutY="293.0" prefHeight="31.0" prefWidth="314.0" />
        <Button layoutX="401.0" layoutY="357.0" mnemonicParsing="false" onAction="#editDone" text="Done">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button layoutX="14.0" layoutY="405.0" mnemonicParsing="false" onAction="#Back" text="Back">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>