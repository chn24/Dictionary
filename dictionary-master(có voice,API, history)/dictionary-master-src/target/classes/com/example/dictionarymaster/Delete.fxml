<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionarymaster.deleteWordController">
    <children>
        <ImageView fitHeight="55.0" fitWidth="850.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/header.png" />
            </image>
        </ImageView>
        <Label layoutX="305.0" layoutY="114.0" text="Delete a word">
            <font>
                <Font size="39.0" />
            </font>
        </Label>
        <Label layoutX="186.0" layoutY="250.0" text="Word">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="deleteWord" layoutX="305.0" layoutY="257.0" prefHeight="31.0" prefWidth="314.0" promptText="Your answer" />
        <Button layoutX="377.0" layoutY="350.0" mnemonicParsing="false" onAction="#deleteDone" prefHeight="45.0" prefWidth="110.0" style="-fx-background-color: #00a2e8 ; -fx-text-fill: #fff; " text="Done">
            <font>
                <Font size="18.0" />
            </font>
         <graphic>
            <ImageView disable="true" fitHeight="28.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/done.png" />
               </image>
            </ImageView>
         </graphic>
        </Button>
        <Button layoutX="14.0" layoutY="405.0" mnemonicParsing="false" onAction="#Back" prefHeight="44.0" prefWidth="110.0" style="-fx-background-color: #00a2e8 ; -fx-text-fill: #fff; " text="Back">
            <font>
                <Font size="20.0" />
            </font>
         <graphic>
            <ImageView disable="true" fitHeight="31.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/back.png" />
               </image>
            </ImageView>
         </graphic>
        </Button>
    </children>
</AnchorPane>
